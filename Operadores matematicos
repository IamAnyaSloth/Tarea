let a = 5;
let b = 3;
let c = 2;

a += 2;
b -= 1;
c *= 3;

let suma = a + b;
let resta = a - b;
let multiplicacion = a * c;
let division = a / c;

console.log("a =", a);
console.log("b =", b);
console.log("c =", c);
console.log("suma =", suma);
console.log("resta =", resta);
console.log("multiplicacion =", multiplicacion);
console.log("division =", division);
